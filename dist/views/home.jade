-
  list = [{"name":"Docs"},{"name":"Home"}]

doctype html
html(lang="en",ng-app="heypi")
  head
    title= message
    link(href='https://ajax.googleapis.com/ajax/libs/angular_material/0.11.2/angular-material.min.css',rel='stylesheet')
    script(src='https://ajax.googleapis.com/ajax/libs/angularjs/1.5.0-beta.2/angular.min.js')
    script(src='https://ajax.googleapis.com/ajax/libs/angularjs/1.4.7/angular-animate.min.js')
    script(src='https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular-aria.min.js')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.15/angular-ui-router.js')
    script(src='https://ajax.googleapis.com/ajax/libs/angular_material/0.11.2/angular-material.min.js')
    style
      include home.css
    script
      include home.js
    
  body(layout="column" ng-controller="AppController as vm")
    div.header
    md-toolbar(layout="row")
      div.md-toolbar-tools
        md-button(ng-click="vm.toggleSidenav('left')", hide-gt-sm class="md-icon-button")
          md-icon(aria-label="Menu", md-svg-icon="https://s3-us-west-2.amazonaws.com/s.cdpn.io/68133/menu.svg")
        h1 HeyPI
    div(flex, layout="row")
      md-sidenav(layout='column',md-component-id="left",md-is-locked-open="$mdMedia('gt-sm')").md-sidenav-left.md-whiteframe-4dp
        md-button(ui-sref="collections").md-accent.md-raised collections
        md-button(ui-sref="docs").md-accent.md-raised docs
        md-button.md-accent.md-raised users
        md-button.md-accent.md-raised usage
      div(layout="column", flex, id="content" ui-view)
      
        
  
